<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sheens.iphone" version="4.10.8" android-versionCode="410080" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Sheens</name>
    <description>
        Sheens application.
    </description>
    <author email="kolarpad@yahoo.com" href="http://estateapps.co.uk/">
        Estate Apps Team
    </author>
    <access origin="*" />
    <allow-navigation href="*" />
    <engine name="ios" spec="https://github.com/apache/cordova-ios.git#4.5.4" />
    <preference name='phonegap-version' value="cli-6.5.0" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="false" />
    <preference name="DisallowOverscroll" value="true" /> 
    <preference name="target-device" value="universal" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="350" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <config-file platform="ios" parent="NSLocationWhenInUseUsageDescription" mode="merge">
        <string>Your current location will be displayed on the map and used for directions, nearby search results and estimated travel times.</string>
    </config-file>
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" source="npm" />
    <plugin name="cordova-plugin-splashscreen" spec="~3.2.0" source="npm" />
    <plugin name="cordova-plugin-contacts" spec="~2.0.1" source="npm" />
    <plugin name="cordova-plugin-device" spec="~1.1.1" source="npm" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3" source="npm" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.1" source="npm" />
    <plugin name="cordova-plugin-screen-orientation" spec="~1.4.0" source="npm" />
    <plugin name="cordova-plugin-network-information" spec="~1.2.0" source="npm" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" source="npm" />
    <plugin name="cordova-plugin-file" spec="~4.1.1" source="npm" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.5.0" source="npm" />
    <plugin platform="android" name="phonegap-plugin-push" spec="~1.9.1" source="npm" />
    <plugin platform="ios" name="phonegap-plugin-push" spec="~1.5.3" source="npm" />
    <plugin name="uk.co.workingedge.phonegap.plugin.istablet" spec="~1.1.0" source="npm" />
    <icon src="www/icon.png" />
    <splash src="www/splash.png" />
    <icon src="res/icon/ios/icon-60@3x.png" width="180" height="180" />
