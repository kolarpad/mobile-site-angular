<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coandlovett.iphone" version="4.11.2" android-versionCode="411020" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Lovett&amp;Co</name>
    <description>
        Lovett and Co application.
    </description>
    <author email="kolarpad@yahoo.com" href="http://estateapps.co.uk/">
        Estate Apps Team
    </author>
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />  
    <allow-intent href="mailto:*" /> 
    <preference name='phonegap-version' value="cli-8.0.0" />
    <preference name="orientation" value="default" />
    <preference name="fullscreen" value="false" />
    <preference name="DisallowOverscroll" value="true" /> 
    <preference platform="ios" name="target-device" value="universal" />
    <preference platform="android" name="android-minSdkVersion" value="16" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference platform="android" name="SplashMaintainAspectRatio" value="true" />
    <preference platform="ios" name="FadeSplashScreenDuration" value="350" />
    <preference platform="ios" name="ShowSplashScreenSpinner" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <platform name="ios">
        <config-file target="*-Info.plist" parent="NSLocationWhenInUseUsageDescription" mode="merge">
            <string>Your current location will be displayed on the map and used for directions, nearby search results and estimated travel times.</string>
        </config-file>
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" source="npm" />
    <plugin name="cordova-plugin-splashscreen" spec="~5.0.2" source="npm" />
    <plugin name="cordova-plugin-contacts" spec="~3.0.1" source="npm" />
    <plugin name="cordova-plugin-device" spec="~2.0.2" source="npm" />
    <plugin name="cordova-plugin-geolocation" spec="~4.0.1" source="npm" />
    <plugin name="cordova-plugin-statusbar" spec="~2.4.2" source="npm" />
    <plugin name="cordova-plugin-screen-orientation" spec="~3.0.1" source="npm" />
    <plugin name="cordova-plugin-network-information" spec="~2.0.1" source="npm" />
    <plugin name="cordova-plugin-inappbrowser" spec="~3.0.0" source="npm" />
    <plugin name="cordova-plugin-file" spec="~6.0.1" source="npm" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.7.1" source="npm" />
    <plugin name="phonegap-plugin-push" spec="2.1.3" source="npm" />
    <platform name="android">
        <resource-file src="google-services.json" target="app/google-services.json" />
    </platform>
    <platform name="android">
        <hook type="after_prepare" src="scripts/android/afterPrepareNotiIcon.js" />
    </platform>
    <plugin name="uk.co.workingedge.phonegap.plugin.istablet" spec="~1.2.0" source="npm" />
    <icon platform="ios" src="res/icon/ios/icon-60@3x.png" width="180" height="180" />
    <icon platform="ios" src="res/icon/ios/icon-60.png" width="60" height="60" />
    <icon platform="ios" src="res/icon/ios/icon-60@2x.png" width="120" height="120" />
    <icon platform="ios" src="res/icon/ios/icon-76.png" width="76" height="76" />
    <icon platform="ios" src="res/icon/ios/icon-76@2x.png" width="152" height="152" />
    <icon platform="ios" src="res/icon/ios/icon-83.5@2x.png" width="167" height="167" />
    <icon platform="ios" src="res/icon/ios/icon-small@3x.png" width="87" height="87" />
    <icon platform="ios" src="res/icon/ios/icon-40.png" width="40" height="40" />
    <icon platform="ios" src="res/icon/ios/icon-40@2x.png" width="80" height="80" />
    <icon platform="ios" src="res/icon/ios/icon-40@3x.png" width="120" height="120" />
    <icon platform="ios" src="res/icon/ios/icon.png" width="57" height="57" />
    <icon platform="ios" src="res/icon/ios/icon@2x.png" width="114" height="114" />
    <icon platform="ios" src="res/icon/ios/icon-72.png" width="72" height="72" />
    <icon platform="ios" src="res/icon/ios/icon-72@2x.png" width="144" height="144" />
    <icon platform="ios" src="res/icon/ios/icon-small.png" width="29" height="29" />
    <icon platform="ios" src="res/icon/ios/icon-small@2x.png" width="58" height="58" />
    <icon platform="ios" src="res/icon/ios/icon-50.png" width="50" height="50" />
    <icon platform="ios" src="res/icon/ios/icon-50@2x.png" width="100" height="100" />
    <icon platform="ios" src="res/icon/ios/icon-1024.png" width="1024" height="1024" />
    <splash platform="ios" src="res/screen/ios/Default@2x~universal~anyany.png" />
</widget>